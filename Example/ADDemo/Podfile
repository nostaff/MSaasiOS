  pod 'SFAdGdtAdapter'
  # 快手广告主适配包
  pod 'SFAdKsAdapter'
  # 京准通广告主适配包
  pod 'SFAdJztAdapter'
  # 百度广告主适配包
  pod 'SFAdBaiduAdapter'
  # sigmob广告主适配包
  pod 'SFAdSigmobAdapter'
  # Mintegral广告主适配包
  pod 'SFAdMtgAdapter'
  # 美数广告主适配包
  pod 'SFAdMsAdapter'
  # Groomer广告主适配包
  pod 'SFAdGromoreAdapter'

